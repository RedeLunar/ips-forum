<ips:template parameters="$data" />
{{if isset( $data['error'] )}}
	{{if \IPS\Member::loggedIn()->modPermission('can_manage_sidebar')}}
		<div class='ps-error'>
			<div class='ps-error-code'>
				{$data['error']}
			</div>
			<div class='ps-error-content'>
				<div class='ps-error-title'>
					{lang="{$data['error_title']}"}
				</div>
				<div class='ps-error-message'>
					{lang="$data['error_message']"}
				</div>
				<span class='ipsType_small'>
					{lang="error_perm"}
				</span>
			</div>
		</div>
	{{endif}}
